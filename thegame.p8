-- clear_cel using neighbour val
-- prefer empty, then non-ground
-- then left neighbour
function clear_cel(x, y)
	val0 = mget(x-1,y)
	val1 = mget(x+1,y)
	if (val0 == 0 or val1 == 0) then
		mset(x,y,0)
	elseif (not fget(val1,1)) then
		mset(x,y,val1)
	else
		mset(x,y,val0)
	end
end

-- test if a point is solid
function solid (x, y)
	if (x < 0 or x >= 128 ) then
		return true
	end
	val = mget(x, y)
	return fget(val, 1)
end

-- test if a point is door
function door (x, y)
	val = mget(x, y)
	return fget(val, 2)
end

function move_pickup(a)
	a.frame = a.f0
end

-- spawn stuff close to x0,y0(player)
function move_spawns(x0, y0)
	for y=y0-10,y0+6 do
		for x=x0-10,x0+10 do
			val = mget(x,y)
			m = nil
			-- pickup
			if (fget(val, 5)) then    
				m = create_actor(2,x+0.5,y+1,1)
				m.f0 = val
				m.frame = val
				if (fget(val,4)) then
					m.ddy = 0 -- zero gravity
				end
			end
			-- monster
			if (fget(val, 3)) then
				m = create_actor(3,x+0.5,y+1,-1)
				m.f0=val
				m.frame=val
			end
			-- boss
			if (fget(val, 7)) then
				m = create_boss(5,x+0.5,y,-1)
				m.f0=val
				m.frame=val
				boss_spawned = true
				boss_spawned_speak = true
				sfx(-1)
				music(-1)
				sfx(4)
			end
			if (fget(val, 4) and boss_death) then
				m = create_flag(6,x,y)
				m.frame = val
			end
			-- clear cel if spawned something
			if (m ~= nil) then
				clear_cel(x,y)
			end
		end
	end
end

function move_player(pl)
	if (pl.life == 0) then
		death_t = 1
		for i=1,32 do
			s=create_sparkle(pl.x, pl.y-0.6, 96, 0)
			s.dx = cos(i/32)/2
			s.dy = sin(i/32)/2
			s.max_t = 30 
			s.ddy = 0.01
			s.frame=96+rnd(3)
			s.col = 7
		end
		del(actor,pl)
		--sfx(16)
		music(-1)
		sfx(0)
		return
	end
	accel = 0.05
	if (not pl.standing) then
		accel = accel / 2
	end
	if (btn(0,b)) then 
		pl.dx = pl.dx - accel; pl.dir=-1
	end
	if (btn(1,b)) then 
		pl.dx = pl.dx + accel; pl.dir=1
	end
	if (btn(2,b) and pl.standing) then
		--solid(pl.x,pl.y)) then 
		pl.dy = -1
		sfx(8)
	end
	--shooting
	if (btn(4,b)) then
		pl.shooting = true
		if pl.dir == -1 then
			shoot_left(pl,53)
		else
			shoot_right(pl,30)
		end
	end
	if(pl.shooting) then
		pl.shooting_frame_rate= pl.shooting_frame_rate + 0.5
	end
	-- frame
	if (pl.standing) then
		pl.f0 = (pl.f0+abs(pl.dx)*2+4) % 4
	else
		pl.f0 = (pl.f0+abs(pl.dx)/2+4) % 4 
	end
	if (abs(pl.dx) < 0.1) then
		if(pl.shooting) then
			if(pl.super > 1) then
				pl.frame = 75 + flr(pl.shooting_frame_rate)
			else
				pl.frame = 91 + flr(pl.shooting_frame_rate)
			end
			--pl.frame = 91 + flr(pl.shooting_frame_rate)
		else
			pl.shooting_frame_rate = 0
			pl.frame = 48 pl.f0=0
		end
	else
		if(pl.shooting) then
			if(pl.super > 1) then
				pl.frame = 123 + flr(pl.shooting_frame_rate)
			else
				pl.frame = 107 + flr(pl.shooting_frame_rate)
			end
			--pl.frame = 107 + flr(pl.shooting_frame_rate)
		else
			pl.shooting_frame_rate = 0
			pl.frame = 49 + flr(pl.f0)
		end
	end
	pl.shooting = false
	if(pl.shooting_frame_rate>3) then
		pl.shooting_frame_rate = 0
	end
end

function move_boss(boss)
	if(player.x-boss.x < -1) then
		boss.dir = -1
	end
	if(player.x-boss.x > 1) then
		boss.dir = 1
	end
	if(shooting_rate % 40 == 1) then
		boss_shooting(boss)
	end
	boss.frame=85
end

function move_monster(m)
	if(player.x-m.x < -3) then
		m.dir = -1
	end
	if(player.x-m.x > 3) then
		m.dir = 1
	end
	m.dx = m.dx + m.dir * 0.02
	m.f0 = (m.f0+abs(m.dx)*3+4) % 4
	m.frame = 112 + flr(m.f0)
	if (m.hit_point <= 2) then
		m.frame = 117 + flr(m.f0)
	end
	if(shooting_rate % 40 == 1) then
		monster_shooting(m)
	end
end

--this function will make all the characters move
function move_actor(actor)
	if (actor.kind == 1) then
		move_player(actor)
	end
	if (actor.kind == 2) then
		move_pickup(actor)
	end
	if (actor.kind == 3) then
		move_monster(actor)
	end
	if (actor.kind == 5) then
		move_boss(actor)
	end
	actor.standing=false
	-- x movement 	
	x1 = actor.x + actor.dx + sgn(actor.dx) * 0.3 
	local broke_block = false
	if(not solid(x1,actor.y-0.5)) then
		actor.x = actor.x + actor.dx
	else -- hit wall
		-- search for contact point
		while (not solid(actor.x + sgn(actor.dx)*0.3, actor.y-0.5)) do
			actor.x = actor.x + sgn(actor.dx) * 0.1
		end
		--if door
		if(door(x1,actor.y-0.5)) then
			if (actor.keys > 0) then
				clear_cel(x1,actor.y-0.5)
				actor.keys -= 1
				sfx(3)
			else
				locked = true
			end
		end
		if (actor.kind == 3) then
			actor.dir = actor.dir * -1
			actor.dx=0
		end
	end
	-- y movement
	if (actor.dy < 0) then
		-- going up
		if (solid(actor.x-0.2, actor.y+actor.dy-1) or solid(actor.x+0.2, actor.y+actor.dy-1)) then
			actor.dy=0 
			-- search up for collision point
			while ( not (solid(actor.x-0.2, actor.y-1) or solid(actor.x+0.2, actor.y-1))) do
				actor.y = actor.y - 0.01
			end
			--if door
			if(door(actor.x-0.2, actor.y+actor.dy-1)) then
				if (actor.keys > 0) then
					clear_cel(actor.x-0.2, actor.y+actor.dy-1)
					actor.keys -= 1
					sfx(3)
				else
					locked = true
				end
			end
			if (door(actor.x+0.2, actor.y+actor.dy-1)) then
				if (actor.keys > 0) then
					clear_cel(actor.x+0.2, actor.y+actor.dy-1)
					actor.keys -= 1
					sfx(3)
				else
					locked = true
				end
			end
		else
			actor.y = actor.y + actor.dy
		end
	else
		-- going down
		if (solid(actor.x-0.2, actor.y+actor.dy) or solid(actor.x+0.2, actor.y+actor.dy)) then
			if(door(actor.x-0.2, actor.y+actor.dy)) then
				if (actor.keys > 0) then
					clear_cel(actor.x-0.2, actor.y+actor.dy)
					actor.keys -= 1
					sfx(3)
				else
					locked = true
				end
			end
			if (door(actor.x+0.2, actor.y+actor.dy)) then
				if (actor.keys > 0) then
					clear_cel(actor.x+0.2, actor.y+actor.dy)
					actor.keys -= 1
					sfx(3)
				else
					locked = true
				end
			end
			-- bounce
			if (actor.bounce > 0 and actor.dy > 0.2) then
				actor.dy = actor.dy * -actor.bounce
			else
				actor.standing=true
				actor.dy = 0
			end
			--snap down
			while (not (solid(actor.x-0.2,actor.y) or solid(actor.x+0.2,actor.y))) do
				actor.y = actor.y + 0.05 
			end
			--pop up even if bouncing
			while(solid(actor.x-0.2,actor.y-0.1)) do
				actor.y = actor.y - 0.05 end
			while(solid(actor.x+0.2,actor.y-0.1)) do
				actor.y = actor.y - 0.05 end
		else
			actor.y = actor.y + actor.dy  
		end
	end
	-- gravity and friction
	actor.dy = actor.dy + actor.ddy
	actor.dy = actor.dy * 0.95
	-- x friction
	if (actor.standing) then
		actor.dx = actor.dx * 0.8
	else
		actor.dx = actor.dx * 0.9
	end
	-- counters
	actor.t = actor.t + 1
end

function monster_shooting(m)
	if(m.dir == -1) then
		create_bullet(m.x-0.6,m.y,0.50,53,0.25)
	else
		create_bullet(m.x+0.9,m.y,0,30,0.25)
	end
end

function boss_shooting(boss)
	if(boss.dir == -1) then
		create_bullet(boss.x-0.6,boss.y-0.5,0.44,53,0.25)
		create_bullet(boss.x-0.6,boss.y-0.5,0.50,53,0.25)
		create_bullet(boss.x-0.6,boss.y-0.5,0.56,53,0.25)
	else
		create_bullet(boss.x+0.9,boss.y-0.5,0.96,30,0.25)
		create_bullet(boss.x+0.9,boss.y-0.5,0,30,0.25)
		create_bullet(boss.x+0.9,boss.y-0.5,0.04,30,0.25)
	end
end

--this is for player
function shoot_right(actor,frame)
	if(actor.super > 0) then
		if(shooting_rate % 5 == 1 or actor.shooting_rate == 3) then
			create_bullet(actor.x+0.9,actor.y,0,frame,0.50)
			s=create_sparkle(actor.x+0.5, actor.y, 30, 9)
			s.dx = s.dx + rnd(0.4)-0.2
			s.dy = s.dy + rnd(0.4)-0.2
			s.max_t = 30
			s.ddy = 0.01
			sfx(10)
			actor.shooting_rate = 0
			actor.super-=1
		end
	end
	if(actor.shooting_rate == 5) then
		create_bullet(actor.x+0.9,actor.y,0,frame,0.50)
		s=create_sparkle(actor.x+0.5, actor.y, 30, 9)
		s.dx = s.dx + rnd(0.4)-0.2
		s.dy = s.dy + rnd(0.4)-0.2
		s.max_t = 30
		s.ddy = 0.01
		sfx(10)
		actor.shooting_rate = 0
	end
end

--this is for player
function shoot_left(actor,frame)
