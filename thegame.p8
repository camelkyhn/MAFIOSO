		if(shooting_rate % 5 == 1 or actor.shooting_rate == 3) then
			create_bullet(actor.x-0.9,actor.y,.50,frame,0.50)
			s=create_sparkle(actor.x+0.5, actor.y, 30, 9)
			s.dx = s.dx + rnd(0.4)-0.2
			s.dy = s.dy + rnd(0.4)-0.2
			s.max_t = 30 
			s.ddy = 0.01 
			sfx(10)
			actor.shooting_rate = 0
			actor.super-=1
		end
	end
	if(actor.shooting_rate == 5) then
		create_bullet(actor.x-0.9,actor.y,.50,frame,0.50)
		s=create_sparkle(actor.x+0.5, actor.y, 30, 9)
		s.dx = s.dx + rnd(0.4)-0.2
		s.dy = s.dy + rnd(0.4)-0.2
		s.max_t = 30 
		s.ddy = 0.01 
		sfx(10)
		actor.shooting_rate = 0
	end
end

function collide(a1, a2)
	if (a1==a2) then
		return
	end
	local dx = a1.x - a2.x
	local dy = a1.y - a2.y
	if(a1.kind == 5) then
		dy = a1.y - 0.5 - a2.y
	end
	if (abs(dx) < a1.w+a2.w) then
		if (abs(dy) < a1.h+a2.h) then
			collide_event(a1, a2)
		end
	end
end

function collide_event(a1, a2)
	if(a1.kind==1) then
		if(a2.kind == 6) then --finish flag
			win = true
		end
		if(a2.kind==2) then --pick up
			-- heavy machine gun
			if (a2.frame==84) then
				a1.super = 100 --ammo
				ak47 = true
				sfx(11)
			end
			-- cigaratte
			if (a2.frame==80) then
				a1.score = a1.score + 1
				cigaratte = true
				sfx(9)
			end
			-- key1
			if (a2.frame==83) then
				a1.keys +=2
				sfx(9)
			end
			-- key2
			if (a2.frame==67) then
				a1.keys +=2
				sfx(9)
			end
			-- key3
			if (a2.frame==66) then
				a1.keys +=2
				sfx(9)
			end
			-- apple
			if (a2.frame==82) then
				a1.score = a1.score + 2
				sfx(9)
			end
			-- cat
			if (a2.frame==81) then
				a1.score = a1.score + 3
				sfx(9)
			end
			del(actor,a2)
		end
		if(a2.kind==3) then -- monster
			if((a1.y-a1.dy) < a2.y-0.7) then
				-- bounce player
				a1.dx = a1.dx * 1.2
				a1.dy = a1.dy * -1.2-- - 0.2
				-- explode
				for i=1,16 do
					s=create_sparkle(a2.x, a2.y-0.5, 96+rnd(3), i%16)
					s.dx = s.dx + rnd(0.4)-0.2
					s.dy = s.dy + rnd(0.4)-0.2
					s.max_t = 30 
					s.ddy = 0.01 
				end
				-- kill monster
				-- to do: in move_monster
				sfx(14)
				del(actor,a2)
			else
				-- player death
				a1.hit_point = 0
				a1.life=0
			end
		end
		if(a2.kind == 4) then --bullet
			if(a1.hit_point == 1) then
				-- player death
				a1.life=0
				a1.hit_point=0
				del(bullets, a2)
				sfx(7)
			else
				a1.hit_point -= 1
				del(bullets, a2)
				sfx(7)
			end
		end
		if(a2.kind == 5) then --boss
			if((a1.y-a1.dy) < a2.y-0.7) then
				-- bounce and back off player
				if(a1.x-a2.x<0) then
					a1.dx = a1.dx * -5
					a1.dy = a1.dy * -1.5
				else
					a1.dx = a1.dx * 5
					a1.dy = a1.dy * -1.5
				end
				if(a2.hit_point-5<1) then
					-- explode
					for i=1,32 do
						s=create_sparkle(a2.x, a2.y-0.6, 96+rnd(3), i%16)
						s.dx = cos(i/32)/2
						s.dy = sin(i/32)/2
						s.max_t = 30 
						s.ddy = 0.01
						s.frame=96+rnd(3)
						s.col = 7
					end
					-- kill boss
					-- to do: in move_boss
					sfx(14)
					del(actor,a2)
					boss_death = true
					win_music = true
				else
					a2.hit_point-=5
				end
			else
				--player death
				a1.hit_point = 0
				a1.life = 0
			end
		end
	end
	if(a1.kind==3) then
		if(a2.kind==4) then -- monster got shoot by a bullet
			if(a1.hit_point == 1) then
				-- explode
				for i=1,16 do
					s=create_sparkle(a1.x, a1.y-0.5, 96+rnd(3), 8)
					s.dx = s.dx + rnd(0.4)-0.2
					s.dy = s.dy + rnd(0.4)-0.2
					s.max_t = 30
					s.ddy = 0.01
				end
				sfx(14)
				del(actor, a1)
				del(bullets, a2)
			else
				a1.hit_point-=1
				del(bullets, a2)
				sfx(15)
			end
		end
	end
	if(a1.kind==5) then
		if(a2.kind==4) then -- boss got shoot by a bullet
			if(a1.hit_point == 1) then
				-- explode
				for i=1,32 do
					s=create_sparkle(a2.x, a2.y-0.6, 96, 2)
					s.dx = cos(i/32)/2
					s.dy = sin(i/32)/2
					s.max_t = 30 
					s.ddy = 0.01
					--s.frame=96+rnd(3)
					--s.col = i%16
				end
				sfx(14)
				del(actor, a1)
				del(bullets, a2)
				boss_death = true
				win_music = true
			else
				a1.hit_point-=1
				del(bullets, a2)
				sfx(15)
			end
		end
	end
end

--this function checks all the important collisions for gameplay
function collisions()
	--collisions between player and the other actors
	for a1 in all(actor) do
		collide(player,a1)
	end
	--collisions between bullets and the other objects
	for bullet in all(bullets) do
		if(solid(bullet.x,bullet.y-0.5)) then
			del(bullets,bullet)
		end
		for a1 in all(actor) do
			collide(a1,bullet)
		end
	end
	--collision if player catches the finish flag
	for a1 in all(flags) do
		collide(player,a1)
	end
end

function move_bullet(bullet)
	bullet.x = bullet.x + bullet.dx
	bullet.y = bullet.y + bullet.dy
	if (bullet.x < 0 or bullet.x > 128) then
		del(bullets, bullet)
	end
end

function move_sparkle(sp)
	if (sp.t > sp.max_t) then
		del(sparkle,sp)
	end
	sp.x = sp.x + sp.dx
	sp.y = sp.y + sp.dy
	sp.dy= sp.dy+ sp.ddy
	sp.t = sp.t + 1
end

--this function is about finish or death
function outgame_logic()
	if (death_t > 0) then
		death_t = death_t + 1
		if (death_t > 30 and btn(4) or btn(5)) then 
			music(-1)
			sfx(-1)
			--sfx(0)
			dpal={0,1,1,2,1,13,6,4,4,9,3,13,1,13,14}       
			-- palette fade
			for i=0,40 do
				for j=1,15 do
					col = j
					for k=1,((i+(j%5))/4) do
						col=dpal[col]
					end
					pal(j,col,1)
				end
				flip()
			end
			-- restart cart end of slice
			run()
		end
	end
	if win then
		win_t+=1
		if (win_t > 30 and btn(4) or btn(5)) then 
			music(-1)
			sfx(-1)
			--music(3,0,68)
			dpal={0,1,1,2,1,13,6,4,4,9,3,13,1,13,14}       
			-- palette fade
			for i=0,40 do
				for j=1,15 do
					col = j
					for k=1,((i+(j%5))/4) do
						col=dpal[col]
					end
					pal(j,col,1)
				end
				flip()
			end
			-- restart cart end of slice
			run()
		end
	end
end

function _update()
	foreach(actor, move_actor)		
	foreach(sparkle, move_sparkle)
	collisions()
	move_spawns(player.x, player.y)
	--shoot
	foreach(bullets, move_bullet)
	outgame_logic()
	t=t+1
	shooting_rate = shooting_rate + 1
	if (player.shooting_rate < 5) then
		player.shooting_rate +=1
	end
	if boss_spawned and not(boss_death) then
		if(t%75 == 74) then
			create_actor(3,126,17,-1)
		end
	end
	if win then
		if(t%10 == 9) then
			for i=1,16 do
				s=create_sparkle(player.x,player.y-0.5, 96+rnd(3), i%16)
				s.dx = s.dx + rnd(0.4)-0.2
				s.dy = s.dy + rnd(0.4)-0.2
				s.max_t = 30 
				s.ddy = 0.01 
			end
		end
		del(actor,player)
		if win_music then
			sfx(-1)
			music(-1)
			music(3,0,68)
			win_music = false
		end
		return
	end
end

function draw_sparkle(s)
	if (s.col > 0) then
		for i=1,15 do
			pal(i,s.col)
		end
	end
	spr(s.frame, s.x*8-4, s.y*8-4)
	pal()
end

function draw_actor(actor)
	if(actor.kind==5) then
		draw_boss(actor)
	else
		spr(actor.frame, actor.x*8-4, actor.y*8-8, 1, 1, actor.dir < 0)
		pal()
	end
end

function draw_boss(boss)
	spr(boss.frame,boss.x*8-4,boss.y*8-16,1,2,boss.dir<0)
	pal()
end

function draw_flag(flag)
	spr(flag.frame,flag.x*8-4,flag.y*8-4)
	pal()
end

function draw_bullet(bullet)
	spr(bullet.frame, bullet.x*8-4, bullet.y*8-4)
	pal()
end

function _draw()
	-- sky
	camera (0,0)
	rectfill (0,0,127,127,1)
	-- clouds behind mountains
	local x = t / 8
	x = x % 128
	local y=0
	mapdraw(16, 48, -x, y, 16, 16, 0)
	mapdraw(16, 48, 128-x, y, 16, 16, 0) 
	local bgcol = 3 -- mountains
	pal(5,bgcol) pal(2,bgcol)
	pal(13,11) -- highlights 
	y = 0
	mapdraw (0, 48, 0, y, 16, 16, 0)
	pal()
	-- map and actors
	cam_x = mid(0,player.x*8-64,1024-128)
	cam_y = mid(0,player.y*6-40,320)
	--cam_y = 84
	camera (cam_x,cam_y)
	pal(12,0)	
	mapdraw (0,0,0,0,128,64,1)
	pal()
	foreach(sparkle, draw_sparkle)
	foreach(actor, draw_actor)
	foreach(bullets, draw_bullet)
	foreach(flags,draw_flag)
	camera(0,0)
	color(7)
	if (death_t > 60) then
		print("press button to revenge!",17,20,8+(t/4)%2)
		print("press button to revenge!",18,20,7)
	end
	if (win_t > 60) then
		print("you won, now you are gf!",17,20,8+(t/4)%2)
		print("you won, now you are gf!",18,20,7)
	end
	if start then
		s=speaks[i_speak]
		continue_print = true
		if not(s == nil) and not(i_print<#s) then
			i_speak+=1
			if(i_speak > 8) then
				start = false
				s = nil
				i_speak = 1
			end
		end
	end
	if boss_spawned_speak then
		s = speaks2[i_speak]
		continue_print = true
		if not(s == nil) and not(i_print<#s) then
			i_speak+=1
			if(i_speak > 2) then
				boss_spawned_speak = false
				s = nil
				i_speak = 1
			end
		end
	end
	if not(s == nil) then
		if(i_print<#s and continue_print) then
			if(t_draw%4 == 3) then
				symbol = sub(s,i_print,i_print)
				if(#str>31) then
					str2 = str2 .. symbol
				else
					str = str .. symbol
				end
				i_print+=1
				if(btn(5)) then
					i_print=#s
					str = ""
					str2 = ""
				end
			end
		else
			i_print = 1
			str = ""
			str2 = ""
			continue_print = false
		end
	end
	cursor(0,2)
	if locked then
		print("locked...")
		if (t_draw%50 == 49) then
			locked = false
		end
	end
	if cigaratte then
		print("ooo alirim bi dal...")
		if (t_draw%50 == 49) then
			cigaratte = false
		end
	end
	if ak47 then
		print("ak-47... simdi onlar dusunsun!")
		if(t_draw%50 == 49) then
			ak47 = false
		end
	end
	print(str)
	cursor(0,10)
	print(str2)
	print("score:"..player.score)
	print("hp:"..player.hit_point)
	if (boss_spawned and not(boss_death)) then
		print("boss hp:" .. boss.hit_point)
	end
	t_draw+=1
end




